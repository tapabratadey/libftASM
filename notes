SRCDIR := src
BINDIR := bin

SRCS := $(wildcard $(SRCDIR)/*.s)
OBJS := $(patsubst $(SRCDIR)/%.s, $(BINDIR)/%.o, $(SRCS))

TARGET := libftasm.a

all: $(TARGET)

clean:
	@echo "Cleaning object files"
	@rm -rf $(BINDIR)

fclean: clean
	@echo "Cleaning binary"
	@rm -f $(TARGET)

re: fclean all

$(TARGET): $(OBJS)
	@echo "LD $@"
	@ar rc $@ $^

$(OBJS): $(BINDIR)/%.o: $(SRCDIR)/%.s
	@mkdir -p $(shell dirname $@)
	@echo "AS $<"
	@nasm -f macho64 -o $@ $<

.PHONY: all clean fclean